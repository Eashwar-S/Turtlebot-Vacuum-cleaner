<launch>
  <arg name="world_file"
       default="$(find turtlebot_vacuum_cleaner)/world/turtlebot.world"/>
  <include file= "$(find turtlebot_gazebo)/launch/turtlebot_world.launch"> 
      <arg name="world_file" 
           value="$(arg world_file)"/>
  </include>
  <node name="walker" 
        pkg="turtlebot_vacuum_cleaner" 
        type="walker"
        output="screen"
        launch-prefix = "xterm -e" />
</launch>
